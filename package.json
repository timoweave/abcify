{
  "name": "abcify",
  "version": "1.0.20",
  "description": "sort object literals, object destructuring, react attributes, and flow type by key",
  "main": "abcify",
  "scripts": {
    "test": "./abcify --switches --imports --destructures --objects --shapes --enums --attributes example/input.js > example/tested.js && diff example/tested.js example/expected.js && rm example/tested.js",
    "test2": "nyc --reporter=html --reporter=text mocha --timeout=3000",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "abc",
    "sort",
    "object literals",
    "es2015 object destructuring",
    "react attributes",
    "flow types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/timoweave/abcify.git"
  },
  "bin": {
    "abcify": "abcify"
  },
  "author": "Timothy Shiu <timoweave@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "argparse": "^1.0.9",
    "flow-parser": "^0.60.1",
    "recast": "^0.12.9",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.2.3",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0"
  }
}
